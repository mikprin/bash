sudo smbclient -U Efim -L 192.168.1.12
mount -t cifs -o username=miksolo //192.168.1.12 /mnt/win_share

user=USER
password=****
domain=192.168.1.12

for fs_tab: //192.168.1.12/foto /mnt/win_share cifs uid=0,credentials=/home/miksolo/.smb,iocharset=utf8,vers=3.0,noperm,_netdev 0 0


